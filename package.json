{
  "name": "todo-app",
  "private": true,
  "version": "0.0.0",
  "main": "dist/main/index.cjs",
  "scripts": {
    "dev": "vite packages/renderer",
    "build:renderer": "vue-tsc -p packages/renderer/tsconfig.json --noEmit && vite build packages/renderer",
    "build:preload": "tsc -p packages/preload/tsconfig.json --noEmit && vite build packages/preload",
    "build:main": "tsc -p packages/main/tsconfig.json --noEmit && vite build packages/main",
    "build": "yarn run build:renderer && yarn run build:preload && yarn run build:main",
    "start": "electron .",
    "dev:electron": "cross-env NODE_ENV=development ts-node scripts/dev-server.ts",
    "start:forge": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "lint": "eslint packages/renderer --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/vue-fontawesome": "^2.0.9",
    "electron-squirrel-startup": "^1.0.0",
    "pinia": "^2.0.27",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "electron": "^22.0.0",
    "eslint": "^8.29.0",
    "eslint-plugin-vue": "^9.8.0",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vue-tsc": "^1.0.9"
  }
}
